{
  "name": "eteeap-survey",
  "private": true,
  "version": "1.0.0",
  "description": "ETEEAP Survey (PHP) - frontend asset build (Tailwind)",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.10"
  },
  "scripts": {
    "build:css": "tailwindcss -c tailwind.config.js -i src/assets/tailwind.css -o public/assets/app.css --minify",
    "watch:css": "tailwindcss -c tailwind.config.js -i src/assets/tailwind.css -o public/assets/app.css --watch"
  }
}
